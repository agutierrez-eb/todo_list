[run]
source =
    todolist_app/models.py
    todolist_app/views.py

omit =
    env/*
    */test*
    todolist_app/apps.py
    manage.py
    todolist/asgi.py
    todolist/wsgi.py
    static/todolist/style.css


[report]
exclude_lines =
    pragma: no cover
    def setUp
    def test_
    return raw_input
    print
    if __name__ == '__main__':
    def suite